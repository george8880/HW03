<h1>Listing users</h1>

<table class="table table-striped table-bordered table-condensed">  
  <thead>  
    <tr>  
      <th>User Name</th>  
      <th>Unique ID</th>  
    </tr>  
  </thead>  
  
  <% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.upid %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
